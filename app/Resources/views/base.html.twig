<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}LA Restaurants{% endblock %}</title>
        {% block stylesheets %}
            <link href="{{ asset('/build/css/bootstrap.min.css') }}" type="text/css" rel="stylesheet">
            <link href="{{ asset('/build/css/component.css') }}" type="text/css" rel="stylesheet">
            <link href="{{ asset('/build/css/style.css') }}" type="text/css" rel="stylesheet">
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        {% block body %}{% endblock %}
        {% block javascripts %}
            <script src="{{ asset('/build/js/bootstrap.min.js') }}"></script>
            <script src="{{ asset('/build/js/modernizr.custom.js') }}"></script>
            <script src="{{ asset('/build/js/toucheffects.js') }}"></script>
            <script src="https://cdn.jsdelivr.net/npm/jquery@3.2.1/dist/jquery.min.js"></script>
            <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAsd26H02HZUMdkbxvU3YY1Cizr8vq5WGo&callback=initMap"></script>
        {% endblock %}
    </body>
</html>
